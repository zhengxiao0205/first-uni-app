{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "百年奥莱",
				"navigationBarBackgroundColor": "#ffffff",
				// "navigationStyle": "custom",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": {
						"buttons": [{
								"float": "left",
								"fontSrc": "static/iconfont.ttf",
								"text": "\ue617"
							},
							{
								"float": "right",
								"fontSrc": "static/iconfont.ttf",
								"text": "\ue665"
							}
						]
					}
				}
			}
		}, {
			"path": "pages/list/list",
			"style": {
				"navigationBarTitleText": "",
				"navigationBarBackgroundColor": "#ffffff",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": {
						"searchInput": {
							"placeholder": "请输入关键字",
							"disabled": true,
							"align": "left",
							"autoFocus": false,
							"borderRadius": "15px",
							"backgroundColor": "#F7F7F7",
							"placeholderColor": "B3B3B3"
						}
					}
				}
			}

		}, {
			"path": "pages/shopcart/shopcart",
			"style": {
				"navigationBarTitleText": "购物车",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": false
				}
			}

		}, {
			"path": "pages/my/my",
			"style": {
				"enablePullDownRefresh": false,
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": false
				}
			}

		}, {
			"path": "pages/search/search",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#ffffff",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": {
						"searchInput": {
							"placeholder": "nike",
							"disabled": false,
							"align": "left",
							"autoFocus": true,
							"borderRadius": "15px",
							"backgroundColor": "#F7F7F7",
							"placeholderColor": "B3B3B3"
						},
						"buttons": [{
							"float": "right",
							"text": "搜索",
							"color": "#636263",
							"fontSize": "16px",
							"width": "60px"
						}]
					}
				}
			}
		}, {
			"path": "pages/search-list/search-list",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#ffffff",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": {
						"searchInput": {
							"placeholder": "nike",
							"disabled": true,
							"align": "left",
							"borderRadius": "15px",
							"backgroundColor": "#F7F7F7",
							"placeholderColor": "B3B3B3"
						},
						"buttons": [{
							"float": "right",
							"text": "筛选",
							"color": "#636263",
							"fontSize": "16px",
							"width": "60px"
						}]
					}
				}
			}

		}, {
			"path": "pages/detail/detail",
			"style": {
				"navigationBarTitleText": "商品详情",
				"app-plus": {
					"scrollIndicator": "none", // 控制app端的首页滚动条
					"titleNView": {
						"type": "transparent",
						"buttons": [{
							"type": "menu"
						}, {
							"type": "share"
						}]
					}
				}
			}
		}, {
			"path": "pages/my-config/my-config",
			"style": {
				"navigationBarTitleText": "设置",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#ffffff",
				"app-plus": {
					"titleNView": {
						"buttons": [{
							"float": "right",
							"type": "menu"
						}]
					}
				}
			}
